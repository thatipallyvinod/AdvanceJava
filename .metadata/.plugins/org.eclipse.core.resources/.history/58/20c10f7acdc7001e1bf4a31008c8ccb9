package LinkedList;
import java.util.*;
public class Demo1 {

	public static void main(String[] args) {
		LinkedList<Integer> ll=new LinkedList<Integer>();
		//addding
		ll.add(10);
		ll.add(20);
		ll.add(30);
		System.out.println(ll);
		
		//adding element to perticular Index
		ll.add(1,15);
		System.out.println(ll);
		
		//adding First 
		ll.addFirst(5);
		System.out.println(ll);
		
		//Adding Last
		ll.addLast(40);
		System.out.println(ll);
		
		//Removing all
		/*ll.clear();
		System.out.println(ll);*/
		
		// Element is Available or not
		
		if(ll.contains(15))
			System.out.println("element is availa"+ll);
		else
			System.out.println("Does not exist");
	}
}